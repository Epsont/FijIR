{
  "name": "FijIR",
  "short_name": "FI",
  "description": "Added button Fiji and IR",
  "author": "Epsont (Ilya Akhmanov) 2GIS company",
  "version": "1.4",
  "manifest_version": 3,
  
  "icons": {
      "16":"icons/16.png",
      "32":"icons/32.png",
      "48":"icons/48.png",
      "128":"icons/128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_area": "navbar",
    "default_title": "FijIR (Подробнее)"
  },
  "content_scripts": [
    {
        "matches": [ 
          "https://2gis.ru/*",
          "https://2gis.by/*",
          "https://2gis.kz/*",
          "https://2gis.kg/*",
          "https://2gis.uz/*",
          "https://2gis.az/*",
          "https://2gis.ua/*",
          "https://2gis.ae/*",
          "https://maps.urbi.ae/*"
        ],
        "js": [ "background.js" ]
    }
  ]
}